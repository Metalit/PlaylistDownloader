{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "PlaylistDownloader",
    "id": "playlistdownloader",
    "version": "0.1.1",
    "url": null,
    "additionalData": {
      "overrideSoName": "libplaylistdownloader.so"
    }
  },
  "workspace": {
    "scripts": {
      "bsml": [
        "pwsh ./scripts/push-bsml.ps1 $0?"
      ],
      "build": [
        "pwsh ./scripts/build.ps1 $0?"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 $0?",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "qmodIncludeDirs": [],
    "qmodIncludeFiles": [],
    "qmodOutput": null
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.6",
      "additionalData": {
        "extraFiles": [
          "src/inline-hook"
        ]
      }
    },
    {
      "id": "paper",
      "versionRange": "^3.6.1",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "*",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.2",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "^3700.0.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.20",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^1.4.2",
      "additionalData": {}
    },
    {
      "id": "playlistcore",
      "versionRange": "^1.4.0",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.2",
      "additionalData": {}
    },
    {
      "id": "beatsaverplusplus",
      "versionRange": "^0.1.3",
      "additionalData": {}
    },
    {
      "id": "web-utils",
      "versionRange": "^0.6.5",
      "additionalData": {}
    }
  ]
}
